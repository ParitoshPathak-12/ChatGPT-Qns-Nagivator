{
    "manifest_version": 3,
    "name": "ChatGPT Qns Navigator",
    "version": "1.0",
    "host_permissions": ["*://*.chatgpt.com/*"],
    "content_scripts": [
        {
            "matches": ["*://*.chatgpt.com/*"],
            "js": ["content.js"],
            "css": ["styles.css"],
            "run_at": "document_idle"
        }
    ]
}