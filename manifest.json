{
    "manifest_version": 3,
    "name": "ChatGPT Qns Navigator",
    "version": "1.0",
    "host_permissions": ["*://*.chatgpt.com/*"],
    "description": "Navigate quickly between questions in ChatGPT by clicking Q1, Q2, Q3 buttons.",
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.chatgpt.com/*"],
            "js": ["content.js"],
            "css": ["styles.css"],  
            "run_at": "document_idle"
        }
    ]
}